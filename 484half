#include <iostream>
#include <string.h>
#include <algorithm>
using namespace std;
int main()
{
	ios::sync_with_stdio(0);
	cin.tie(0);
	char a[10000],b[]="0123456789",c[10]={};
	cin.getline(a,sizeof(a));
	for(int i=0;i<strlen(a);i+2){
		for(int j=0;j<10;j++){
			if(a[i]==b[j])c[j]++;
		}
	}
	for(int i=0;i<10;i){
		if(c[i]!=0){
			cout<<i<<" "<<c[i]<<'\n';
		}
	}
	
	
	return 0;
}
