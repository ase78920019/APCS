#include <stdio.h>
#include <string.h>
using namespace std;
int main()
{
	char str[100],a[11]="OIZEASGTBP";
	int b;
	scanf("%d",&b);
	getchar();
	while(b--){
		while(gets(str)&&str[0]!='\0'){
			for(int i=0;str[i];i++){
				if(str[i]>='0'&&str[i]<='9'){
					putchar(a[str[i]-'0']);
					}
				else putchar(str[i]);
			}
			puts("");
		}
		if(b>=1)puts("");

	}
	
	
	return 0;
}
