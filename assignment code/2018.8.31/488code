#include<iostream>
using namespace std;
int main()
{
    ios::sync_with_stdio(0);
    cin.tie(0);
    int n,a,b;
    cin>>n;
    for(int i=0;i<n;i++){
    	cin>>a>>b;
    	for(int o=1;o<=b;o++){
    		for(int j=1;j<=a;j++){
    			for(int k=1;k<=j;k++){
    				cout<<j;
    			}
    			cout<<'\n';
    		}
    		for(int j=a-1;j>0;j--){
    			for(int k=0;k<j;k++){
   	 			cout<<j;
    			}
    			cout<<'\n';
    		}
    		if(i==n-1&&o==b){
			}
    		else cout<<'\n';
    	}
	}


    return 0;
}
