#include <iostream>
#include <cmath>
#include <iomanip>
using namespace std;

int main(void) {
	ios::sync_with_stdio(0);
	cin.tie(0);
	int n,c=0;
	double a,b;
	cin>>n;
	while(n--){
		cin>>a>>b;
		a=(a*9.00/5.00)+32.00;
		a+=b;
		a=(a-32.00)*5.00/9.00;
		c++;
		cout<<fixed<<setprecision(2)<<"Case "<<c<<": "<<a<<"\n";
	}

	return 0;
}
