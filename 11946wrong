#include <stdio.h>
#include <string.h>
using namespace std;
int main()
{
	char str[100],a[11]="OIZEASGTBP";
	int b;
	scanf("%d", &b);
	while(b--){
		while(gets(str)){
			if(str[0]=='\0')break;
			for(int i=0;strlen(str);i++){
				if(str[i]>='0'&&str[i]<='9'){
					str[i]=a[str[i]-'0'];
				}
			}
			puts(str);
		}
		if(b)puts("");
	}
	
	
	return 0;
}
