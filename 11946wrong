#include <stdio.h>
#include <iostream>
#include <string.h>
using namespace std;
int main()
{
	ios::sync_with_stdio(0);
	cin.tie(0);
	char str[100],a[11]="OIZEASGTBP";
	int b;
	cin>>b;
	while(b--){
		while(gets(str)){
			if(str[0]=='\0')break;
			for(int i=0;str[i];i++){
				if(str[i]>='0'&&str[i]<='9'){
						str[i]=a[str[i]-'0'];
					}
			}
			puts(str);
		}
		if(b==1)puts("");

	}
	
	
	return 0;
}
