#include <stdio.h>
#include <iostream>
#include <string.h>
using namespace std;
int main()
{
	ios::sync_with_stdio(0);
	cin.tie(0);
	int a[100005],b[11]={};
	int *c[11];
	int x=0,o=1,p=1;
	while(cin>>a[x]){
		for(int i=0;i<11;i++){
			if(a[x]==i){
				b[a[x]]++;
				if(b[a[x]]==1){
					c[o]=&b[a[x]];
					o++;
					p++;
				}
			}
		}
		x++;
	}
	for(int i=1;i<=o;i++){
		for(int j=0;j<10;j++){
			if(c[i] == &b[j] && b[j]>0){
				printf("%d %d",j,b[j]);
				p--;
				if(p>1)printf("\n");
			}	
		}
	}
	
	
	
	return 0;
}
