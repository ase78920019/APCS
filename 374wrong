#include<bits/stdc++.h>
using namespace std;

long long int times(long long int b,long long int p)
{
	if(p==0)
		return 1;
	if(p%2!=0)
		return b*times(b,p-1);
	long long t=times(b,p/2);
	return t*t;
}
int main(){
	ios::sync_with_stdio(0);
	cin.tie(0);
	long long int b,p,m;
	while(cin>>b>>p>>m){
		cout<<times(b,p)%m<<"\n";
	}
	
	return 0;
}
